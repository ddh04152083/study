<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ddh.mapper.LoginMapper">

    <select id="selectLogin" resultType="com.ddh.pojo.Login">
        select * from login
    </select>
    <select id="queryById" resultType="com.ddh.pojo.Login">
        select * from login where id =#{id}
    </select>
</mapper>